{
  "extends": "stylelint-config-standard",
  "ignoreFiles": ["index-dist.css"],
  "rules": {
    "declaration-colon-newline-after": "always-multi-line",
    "selector-type-no-unknown": null,
    "property-no-unknown": [
      true,
      {
        "ignoreProperties": [
          "content-visibility"
        ]
      }
    ]
  }
}